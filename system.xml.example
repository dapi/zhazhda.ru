    <views>
      <view name="internal" class="dpl::Web::View::Internal"/>
      <view name="tt2" dir="/home/danil/projects/nz/templ/"
        class="dpl::Web::View::TT2" nocache="1" maxage="1"> <!-- code="${result:code}|200" -->
        <options>
          <COMPILE_DIR>/tmp/ttc/</COMPILE_DIR>
          <COMPILE_EXT>.ttc</COMPILE_EXT>
          <INTERPOLATE>1</INTERPOLATE> <!-- Позволяет использовать $vasya вместо [% vasya %] -->
          <POST_CHOMP>0</POST_CHOMP>
          <PRE_CHOMP>0</PRE_CHOMP>
          <RELATIVE>1</RELATIVE>
          <ABSOLUTE>1</ABSOLUTE>
          <TRIM>1</TRIM> <!--	# Удаляет CR/LF -->
          <AUTO_RESET>0</AUTO_RESET>
        </options>
        <header>
